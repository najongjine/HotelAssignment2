{
  "ConnectionStrings": { //ConnectionStrings 이 이름은 정해져 있는 이름임
    //DefaultConnection 얘는 내가 정해도 되는 이름임
    //Database= 내가 정할 스키마 이름
    "DefaultConnection": "Server=(LocalDb)\\MSSQLLocalDB;Database=HiddenVilla;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "APISettings": {
    "SecretKey": "mySecretKey#$%#$@@",
    "ValidAudience": "https://localhost:5001;http://localhost:5000",
    "ValidIssuer": "https://localhost:5001;http://localhost:5000"
  }
}
